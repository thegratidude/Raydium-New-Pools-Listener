# Automated Trading Configuration
# Copy these variables to your .env file and modify as needed

# Enable/disable automated trading
AUTO_TRADING_ENABLED=true

# Trading parameters
TRADE_SOL_AMOUNT=0.05
TRADE_SLIPPAGE=5

# Rate limiting
MAX_TRADES_PER_HOUR=10

# Solana wallet configuration (should already be in your .env)
# SOLANA_SECRET=your_wallet_secret_key_here 